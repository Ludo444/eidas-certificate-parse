{
    "name": "liamdennehy/eidas-certificate",
    "description": "eIDAS Trust Services and Qualified Certificate Statement (QCStatement) library",
    "type": "library",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/liamdennehy/xmlseclibs.git"
        }
    ],
    "require": {
        "phpseclib/phpseclib": "^2.0",
        "robrichards/xmlseclibs": "dev-mgf1#c2e5a1424804c462ec5342bb68858725cbe78256",
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3",
        "phpunit/phpunit": "^7",
        "sop/asn1": "^3.4"
    },
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Liam Dennehy",
            "email": "liam@wiemax.net"
        }
    ],
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "config": {
        "platform": {
            "php": "7.1.1"
        }
    }
}
